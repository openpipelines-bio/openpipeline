    else
      VIASH_TEST_GEX_INPUT="$VIASH_TEST_GEX_INPUT;""$(ViashDockerStripAutomount "$var")"
    fi
    done
    VIASH_PAR_GEX_INPUT="$VIASH_TEST_GEX_INPUT"
  fi
  if [ ! -z "$VIASH_PAR_ABC_INPUT" ]; then
