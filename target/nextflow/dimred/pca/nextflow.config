  docker.enabled = true
  docker.runOptions = "-i -v ${baseDir}:${baseDir}"
  process.container = "dataintuitive/viash"
  params {
    pca__input = "viash_no_value"
    pca__output = "viash_no_value"
    pca__nPCs = "25"
    id = ""
    testScript = "run_test.sh"
    testResources = [ "run_test.sh", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5ad" ]
    pca {
      name = "pca"
      container = "dimred_pca"
      containerTag = "main_build"
      containerRegistry = "openpipeline"
      command = "pca"
      tests {
        isDefined = true
        testScript = "run_test.sh"
        testResources = [ "run_test.sh", "../../../resources/test/pbmc_1k_protein_v3_filtered_feature_bc_matrix.h5ad" ]
      }
      arguments {
        input {
          name = "input"
          otype = "--"
          required = true
          type = "file"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.pca__input}"
          description = "Input h5ad file"
        }
        output {
          name = "output"
          otype = "--"
          required = true
          type = "file"
          direction = "Output"
          multiple = false
          multiple_sep = ":"
          value = "${params.pca__output}"
          description = "Output h5ad file containing the dimred object."
        }
        nPCs {
          name = "nPCs"
          otype = "--"
          required = false
          type = "integer"
          direction = "Input"
          multiple = false
          multiple_sep = ":"
          value = "${params.pca__nPCs}"
          dflt = "25"
          description = "The number of principal components to calculate."
        }
      }
    }
  }