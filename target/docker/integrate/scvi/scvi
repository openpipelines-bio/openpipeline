fi
if [[ -n "$VIASH_PAR_N_VAR_MIN_COUNT" ]]; then
  if ! [[ "$VIASH_PAR_N_VAR_MIN_COUNT" =~ ^[-+]?[0-9]+$ ]]; then
    ViashError '--n_var_min_count' has to be an integer. Use "--help" to get more information on the parameters.
