  VIASH_PAR_OUTPUT=$(ViashStripAutomount "$VIASH_PAR_OUTPUT")
fi
if [ ! -z "$VIASH_PAR_OUTPUT_MODEL" ]; then
  VIASH_PAR_OUTPUT_MODEL=$(ViashStripAutomount "$VIASH_PAR_OUTPUT_MODEL")
fi
if [ ! -z "$VIASH_META_RESOURCES_DIR" ]; then
