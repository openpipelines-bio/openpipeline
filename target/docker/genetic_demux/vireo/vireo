            VIASH_PAR_GENO_TAG=$(ViashRemoveFlags "$1")
            shift 1
            ;;
        --no_doublet)
            [ -n "$VIASH_PAR_NO_DOUBLET" ] && ViashError Bad arguments for option \'--no_doublet\': \'$VIASH_PAR_NO_DOUBLET\' \& \'$2\' - you should provide exactly one argument for this option. && exit 1
