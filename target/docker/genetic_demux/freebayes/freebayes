fi

# detect volumes from file arguments
VIASH_CHOWN_VARS=()
if [ ! -z "$VIASH_PAR_BAM" ]; then
  VIASH_EXTRA_MOUNTS+=( "$(ViashAutodetectMountArg "$VIASH_PAR_BAM")" )
  VIASH_PAR_BAM=$(ViashAutodetectMount "$VIASH_PAR_BAM")
fi
if [ ! -z "$VIASH_PAR_BAM_LIST" ]; then
  VIASH_EXTRA_MOUNTS+=( "$(ViashAutodetectMountArg "$VIASH_PAR_BAM_LIST")" )
  VIASH_PAR_BAM_LIST=$(ViashAutodetectMount "$VIASH_PAR_BAM_LIST")
fi
if [ ! -z "$VIASH_PAR_FASTA_REFERENCE" ]; then
  VIASH_EXTRA_MOUNTS+=( "$(ViashAutodetectMountArg "$VIASH_PAR_FASTA_REFERENCE")" )
  VIASH_PAR_FASTA_REFERENCE=$(ViashAutodetectMount "$VIASH_PAR_FASTA_REFERENCE")
fi
if [ ! -z "$VIASH_PAR_FASTA_REFERENCE_INDEX" ]; then
  VIASH_EXTRA_MOUNTS+=( "$(ViashAutodetectMountArg "$VIASH_PAR_FASTA_REFERENCE_INDEX")" )
